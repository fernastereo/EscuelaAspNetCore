@*
@model List<ObjetoEscuelaBase>
*@
<table class="table table-dark table-hover">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var objeto in Model)
        {
            <tr>
                <td>@objeto.Nombre</td>
                <td>@objeto.Id</td>
            </tr>
        }
    </tbody>
</table>